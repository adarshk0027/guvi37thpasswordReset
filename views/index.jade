extends layout

block content
  .container.my-5
    .email.row.mx-auto 
      .col-sm-12 
        .form 
          form(action="/verification", method="post") 
          .Otp.my-2 
            label(for="otp") Enter Otp
          .my-5.text 
            input(type="text", name="OtpString",placeholder="Enter A string", id="OtpString")
          .col-sm-12.col-md-6   
            input.bg-primary.my-4.button(type='submit',value='Verify',data-transition='fade', data-theme='c')


      

  
  
  
  
  
  
